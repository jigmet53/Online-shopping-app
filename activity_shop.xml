        android:layout_columnWeight="1"

        />
    <ImageButton
        android:layout_width="0dp"
        android:id="@+id/i6"
        android:onClick="click"
        android:layout_height="0dp"
        android:src="@drawable/j6"
        android:scaleType="centerCrop"
        android:layout_rowWeight="1"
        android:layout_columnWeight="1"


        />

    <ImageButton
        android:layout_width="0dp"
        android:id="@+id/i7"
        android:layout_height="0dp"
